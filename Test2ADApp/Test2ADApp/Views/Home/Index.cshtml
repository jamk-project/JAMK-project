@{
    ViewData["Title"] = "Home Page";
}

    <div class="row">
        <div class="col-md-12">
            <h1>Welcome home page!</h1>

            <div class="row">
                <div class="col-xs-12">
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th>Claim</th>
                                <th>Value</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var claim in Model)
                            {
                                <tr>
                                    <td><b>@claim.Type</b></td>
                                    <td>@claim.Value</td>
                                </tr>
                            }

                        </tbody>
                    </table>
                </div>
            </div>
            
        </div>
    </div>